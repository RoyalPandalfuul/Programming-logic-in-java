package ListaD;

import java.util.Scanner;

public class D08 {

	public static void main(String[] args) {
		
		double n1, n2, n3;
		Scanner input = new Scanner(System.in);
		
		System.out.print("Digite o primeiro numero: ");
		n1 = input.nextDouble();
		System.out.print("Digite o primeiro numero: ");
		n2 = input.nextDouble();
		
		if(n1 > n2){
		n3 = n1 / n2;
		System.out.print("O Resultado de " + n1 + " Dividido por " + n2 + " É " + n3);
		}
		
		if(n1 < n2){
			n3 = n2 / n1;
			System.out.print("O Resultado de " + n2 + " Dividido por " + n1 + " É " + n3);
			}
			
	}

}
